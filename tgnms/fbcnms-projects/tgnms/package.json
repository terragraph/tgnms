{
  "name": "@fbcnms/tg-nms",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run start:dev",
    "start:dev": "nodemon --delay 80ms --exec 'sudo fuser -k -s 9229/tcp; node -r @fbcnms/babel-register --inspect' --ignore '**/__tests__/*' -w scripts/server -w server/ -w .settings/ scripts/server",
    "start:debugging": "LOG_LEVEL=debug SQLITE_DB=test.sqlite PORT=8081 node --require '@fbcnms/babel-register' scripts/server.js",
    "start:prod": "NODE_ENV=production node -r @fbcnms/babel-register scripts/server",
    "build": "rimraf dist && NODE_ENV=production node -r '@fbcnms/babel-register' ../../node_modules/webpack/bin/webpack --config ./config/webpack.production.config.js --progress --profile --colors",
    "eslint": "./node_modules/.bin/eslint --ignore-path .eslintignore",
    "sequelize": "./node_modules/.bin/sequelize --seeders-path server/seeders --migrations-path server/migrations $*",
    "gen-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path server/migrations/",
    "precommit": "./node_modules/.bin/lint-staged",
    "thrift-to-flow": "thrift2flow --path $(pwd)/shared/types/ thrift/*.thrift && mv thrift/*.js shared/types/",
    "test": "(cd ../../ && yarn run test)",
    "test:app": "(cd ../../ && yarn run test app)",
    "test:server": "(cd ../../ && yarn run test server)",
    "test:coverage": "(cd ../../ && yarn run test --all --coverage --collectCoverageFrom='[\"**/fbcnms-projects/tgnms/**/*.js\",\"!**/__tests__/**\", \"!**/__mocks__/**\", \"!**/thrift/gen-nodejs/**\", \"!**/fbcnms-projects/tgnms/shared/types/**/*\", \"!**/fbcnms-projects/tgnms/server/seeders/*\", \"!**/fbcnms-projects/tgnms/server/migrations/*\", \"!**/fbcnms-projects/tgnms/static/*\"]' --coverageReporters=text)",
    "gen-swagger": "swagger-jsdoc -d config/swaggerDef.js -o static/doc/swagger.json",
    "start:remoteoverlaytest": "nodemon -w scripts/remoteOverlayTest/ scripts/remoteOverlayTest/testserver --exec 'node -r @fbcnms/babel-register'"
  },
  "dependencies": {
